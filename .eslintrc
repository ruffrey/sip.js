{
  "root": true,
  "rules": {
    "consistent-return": [0],
    "id-length": [0],
    "no-param-reassign": [1, { "props": false }],
    "spaced-comment": [0],
    "comma-dangle": [0],
    "func-names": [0],
    "padded-blocks": [0],
    "no-use-before-define": [0],
    "no-case-declarations": [0],
    "newline-per-chained-call": [0],
    "prefer-rest-params": [0],
    "no-underscore-dangle": [0],
    "prefer-template": [0]
  },
  "ecmaFeatures": {
    # "experimentalObjectRestSpread": [0],
    "jsx": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base"
}
