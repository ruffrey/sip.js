{
  "root": true,
  "rules": {
    "no-use-before-define": [0],
    "padded-blocks": [0],
    "no-param-reassign": [1, { "props": false }],
    "comma-dangle": [0],
    "prefer-rest-params": [0],
    "no-underscore-dangle": [0],
    "spaced-comment": [0],
    "consistent-return": [0],
    "prefer-template": [1]
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": [0],
    "jsx": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base"
}
